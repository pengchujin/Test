{
  "compilerOptions": {
    "lib": ["esnext", "esnext.asynciterable", "es5", "es6","es2017"],
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "lib",
    "sourceMap": true
  },
  "include": [
    "src/**/*",
    "configs/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}